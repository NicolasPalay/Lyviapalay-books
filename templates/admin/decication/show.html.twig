{% extends 'baseAdmin.html.twig' %}

{% block title %}Decication{% endblock %}

{% block body %}
    <h1>Decication</h1>
<div class="container lp-bornes my-5">
    <table class="table">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ decication.id }}</td>
            </tr>
            <tr>
                <th>Address</th>
                <td>{{ decication.address }}</td>
            </tr>
            <tr>
                <th>DateDedication</th>
                <td>{{ decication.dateDedication ? decication.dateDedication|date('Y-m-d H:i:s') : '' }}</td>
            </tr>
            <tr>
                <th>DateDedicationEnd</th>
                <td>{{ decication.dateDedicationEnd ? decication.dateDedicationEnd|date('Y-m-d H:i:s') : '' }}</td>
            </tr>
        </tbody>
    </table>

    <a href="{{ path('admin_decication_index') }}">back to list</a>

    <a href="{{ path('admin_decication_edit', {'id': decication.id}) }}">edit</a>

    {{ include('admin/decication/_delete_form.html.twig') }}
</div>
{% endblock %}
